{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "\"A full stack document management system, complete with roles and privileges\"",
  "main": "index.js",
  "scripts": {
    "build:dist": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./server/test,./client/test,./screenshots,./reports,./migrations,./coverage,docs,testSetup.js --copy-files",
    "start": "webpack -p && babel-node server.js",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover _mocha -- server/tests/**/*.test.js testSetup.js client/test/**/*.test.js --timeout 30000",
    "client:test:e2e": "./node_modules/.bin/nightwatch --config nightwatch.config.js",
    "client:test": "mocha --reporter progress testSetup.js client/test/**/*.test.js",
    "cover": "rm -rf coverage/ && NODE_ENV=test babel-node ./node_modules/babel-istanbul/lib/cli.js cover _mocha -- -R spec server/tests/**/*.test.js",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "webpack": "webpack --config ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andela-mugorji/Document-Management-System.git"
  },
